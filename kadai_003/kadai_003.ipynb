      "source": [
        "import numpy as np\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "result = np.dot(A, B)\n",
        "\n",
        "# A*Bの積を表示\n",
        "print(result)\n",
        "\n",
        "# 要素内の最大値を表示\n",
        "print(f\"要素の最大値は{result.max()}です\")"
      ]
    }
